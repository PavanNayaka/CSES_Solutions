import java.util.*;
class Main2 {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int[] a=new int[n];
    for(int i=1;i<=n-1;i++){
        a[(sc.nextInt())-1]=1;
    }
    for(int i=0;i<=n-1;i++){
        if(a[i]==0){
            System.out.print(i+1);
        }
    }
}
}
